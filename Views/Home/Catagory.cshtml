    <div class="left-side-bar">
        <div class="brand-logo">
            <a href="index.html">
                <img src="~/vendors/images/deskapp-logo.svg" alt="" class="dark-logo" />
                <img src="~/vendors/images/deskapp-logo-white.svg"
                     alt=""
                     class="light-logo" />
            </a>
            <div class="close-sidebar" data-toggle="left-sidebar-close">
                <i class="ion-close-round"></i>
            </div>
        </div>
        <div class="menu-block customscroll">
            <div class="sidebar-menu">
                <ul id="accordion-menu">
                    <div class="dropdown">
                        <a href="@Url.Action("Admin", "Home")" class="dropdown-toggle" type="button" id="formsDropdown">
                            <span class="micon bi bi-textarea-resize"></span><span class="mtext ml-2">Admin</span>
                        </a>
                    </div>

                    <div class="dropdown">
                        <a href="javascript:;" class="dropdown-toggle" type="button" id="formsDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="micon bi bi-textarea-resize"></span><span class="mtext ml-2">Catagory</span>
                        </a>
                        <div class="dropdown-menu w-100" aria-labelledby="formsDropdown" style="background-color: white;">
                            <a class="dropdown-item" href="@Url.Action("Catagory", "Home")">Add Catagory</a>
                            <a class="dropdown-item" href="@Url.Action("ShowCatagory", "Home")">Show Catagory</a>

                        </div>
                    </div>
                    <div class="dropdown">
                        <a href="javascript:;" class="dropdown-toggle" type="button" id="formsDropdown" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                            <span class="micon bi bi-textarea-resize"></span><span class="mtext ml-2">Product</span>
                        </a>
                        <div class="dropdown-menu w-100" aria-labelledby="formsDropdown" style="background-color: white;">
                            <a class="dropdown-item" href="@Url.Action("Index", "Products")">Show Product</a>
                            <a class="dropdown-item" href="@Url.Action("Create", "Products")">Add Pdoduct</a>

                        </div>
                    </div>
                    <div class="dropdown">
                        <a href="@Url.Action("ShowOrder", "Home")" class="dropdown-toggle" type="button" id="formsDropdown">
                            <span class="micon bi bi-textarea-resize"></span><span class="mtext ml-2">Order</span>
                        </a>
                    </div>
                </ul>
            </div>
        </div>
    </div>

    <style>
        /* Custom CSS for styling the card */
        .custom-card {
            max-width: 400px;
            margin: 0 auto;
            padding: 20px;
            box-shadow: 0px 0px 10px rgba(0, 0, 0, 0.1);
            background-color: white;
        }
        .error{
            color:red;
        }
    </style>
    
    <div class="d-flex justify-content-center align-items-center vh-100">
        <div class="custom-card">
            @using (Html.BeginForm("AddCatagory", "Home", FormMethod.Post, new { @class = "text-center" }))
            {
                <div class="mb-3 mt-3">
                    <label for="email" class="form-label">Catagory Name:</label>
                    @Html.ValidationMessageFor(x=>x.Catagory_name,"",new {@class="error"})
                    @Html.TextBoxFor(x => x.Catagory_name, new { @class = "form-control" })
                </div>

                <button type="submit" class="btn btn-primary">Submit</button>
            }
        </div>
    </div>


    <!-- welcome modal end -->
    <!-- js -->

    <script src="~/vendors/scripts/core.js"></script>
    <script src="~/vendors/scripts/script.min.js"></script>

    <script src="~/vendors/scripts/process.js"></script>
    <script src="~/vendors/scripts/layout-settings.js"></script>
    <script src="~/src/plugins/apexcharts/apexcharts.min.js"></script>
    <script src="~/src/plugins/datatables/js/jquery.dataTables.min.js"></script>
    <script src="~/src/plugins/datatables/js/dataTables.bootstrap4.min.js"></script>
    <script src="~/src/plugins/datatables/js/dataTables.responsive.min.js"></script>
    <script src="~/src/plugins/datatables/js/responsive.bootstrap4.min.js"></script>
    <script src="~/vendors/scripts/dashboard.js"></script>
